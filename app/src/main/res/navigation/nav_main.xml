<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_main"
    app:startDestination="@id/testFragment">

    <fragment
        android:id="@+id/testFragment"
        android:name="com.example.testmvvm.smartsport.ui.fragment.main.first.TestFragment"

        >
        <action
            android:id="@+id/action_testFragment_to_testSecondFragment"
            app:destination="@id/testSecondFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popUpTo="@id/testFragment"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/testSecondFragment"
        android:name="com.example.testmvvm.smartsport.ui.fragment.main.second.TestSecondFragment"
        />

</navigation>